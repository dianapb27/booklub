
<div class="container">
  <h1>Create A Club</h1>
  <%= simple_form_for(@club) do |f| %>
  <%= f.input :name%>
  <%= f.input :description%>
  <%= f.association :book%>
  <%= f.input :language%>
  <%= f.input :cover_photo, as: :file  %>
  <%= f.button :submit, class: "btn btn-outline-success" %>
  <% end %>
</div>

